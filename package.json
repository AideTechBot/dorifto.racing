{
  "name": "dorifto.racing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "cp ./node_modules/htmx.org/dist/htmx.min.js ./public/htmx.min.js && bun --hot run src/index.tsx",
    "setup-stag": "sh ./scripts/setup.sh dorifto-stag",
    "update-stag": "sh ./scripts/update.sh dorifto-stag",
    "lint": "prettier ./src --write",
    "release": "standard-version"
  },
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.22",
    "bun-types": "latest",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "croner": "^7.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "hono": "^3.6.0",
    "htmx.org": "^1.9.5",
    "nanoid": "^5.0.1"
  }
}
